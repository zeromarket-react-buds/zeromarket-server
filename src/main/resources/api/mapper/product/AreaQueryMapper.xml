<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zeromarket.server.api.mapper.product.AreaQueryMapper">

  <select id="getLegalDongIdByLegalCode" resultType="Long">
    SELECT legal_dong_id
    FROM legal_dong_areas
    WHERE legal_dong_code = #{code}
    AND deleted_at IS NULL
<!--    LIMIT 1-->
  </select>

</mapper>
